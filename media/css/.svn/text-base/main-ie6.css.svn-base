#site-menu li a { float: left; }
#twitter-lista .twitter-data { width: 85px; }
#twitter-lista li { float: right; padding: 0 0 6px 0; margin: 5px 4px; width: 100%; }
#twitter-lista ul { width: 292px; } 

#destaques-menu { z-index: 2; }
.destaques-container { z-index: 1; position: absolute; }
#destaques-menu li.selected { filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/media/images/destaques-menu-background.png',sizingMethod='scale'); background-image: none; } 
#destaques-menu li a { font-weight: bold !important }
#destaques-produtos, #destaques-servicos { margin-left: 15px; }
.seta-esquerda, .seta-direita {	top: 95px; }

.seta-esquerda { filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/media/images/destaques-setas-esquerda.png',sizingMethod='scale'); background-image: none; cursor: pointer; }

.seta-direita { filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/media/images/destaques-setas-direita.png',sizingMethod='scale'); background-image: none; cursor: pointer; }

 #noticias-lista li { zoom: 1; }

.input { background-image: none !important; }
#site-footer { zoom:1; background-image: url(/media/images/bottom-background.gif); }
#site-menu-inferior { float: left }
#site-midias-sociais h3 { display: block; float: none; text-align: right; }
#site-midias-sociais li { margin: 0 4px; }

#conteudo-editavel { zoom: 1; } 
#galeria-videos li a span { left: 0; background-image: url(/media/images/video-overlay.gif); } 

#galeria-anexos { clear: left; overflow: hidden; }
#galeria-anexos li a { float: left; }
#galeria-anexos li a span { width: 200px; } 

#produtos, #servicos { overflow: hidden; zoom: 1; } 

#site-footer { float: left; margin: 0; }
.site-wrap { padding-bottom: 40px; } 
#site-footer .site-wrap { padding-bottom: 0 !important; }

#categorias-list li{
	padding: 10px 0 10px 10px;
}

#contato .left-column { width: 450px; } 

#tipo-contato, #tipo-orcamento {
border: none !important;
clear:none;
float:left;
margin: 0 8px 0 0 !important;
width:auto;
}

.formulario input.button { width: 150px !important; }

